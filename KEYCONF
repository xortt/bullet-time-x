addkeysection "Bullet Time X" BulletTimeActions

addmenukey "Start/Stop Bullet Time" bullettime
alias bullettime "netevent bt_activate"
defaultbind b bullettime

addmenukey "Shootdodge" bullettime_dodge
alias bullettime_dodge "netevent bt_activate_dodge"
defaultbind v bullettime_dodge

alias bullettime_handler_reload "netevent bt_handler_reload"
alias bullettime_handler_remove "netevent bt_handler_remove"